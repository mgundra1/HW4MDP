<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nomorenagginginator"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_image"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.csc6999_mobileappdev.no_more_nagging_inator_app.HomePageActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.csc6999_mobileappdev.no_more_nagging_inator_app.ChildrenListActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.csc6999_mobileappdev.no_more_nagging_inator_app.ChildActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.csc6999_mobileappdev.no_more_nagging_inator_app.ChoresListActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.csc6999_mobileappdev.no_more_nagging_inator_app.ChoreActivity"
            android:label="@string/app_name" >
        </activity>

    </application>

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
</manifest>